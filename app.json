{
  "expo": {
    "name": "PicPay",
    "slug": "picpay",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#21c25e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rodrigosegatto.clonepicpay"
    },
    "android": {
      "package": "com.rodrigosegatto.clonepicpay"
    }
  }
}
